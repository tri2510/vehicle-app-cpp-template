{
    "manifestVersion": "v3",
    "name": "QuickBuildApp",
    "interfaces": [
        {
            "type": "vehicle-signal-interface",
            "config": {
                "src": "https://github.com/COVESA/vehicle_signal_specification/releases/download/v4.0/vss_rel_4.0.json"
            }
        },
        {
            "type": "pubsub",
            "config": {
                "reads": [
                    "quickbuild/config",
                    "quickbuild/input"
                ],
                "writes": [
                    "quickbuild/output",
                    "quickbuild/status",
                    "quickbuild/logs"
                ]
            }
        }
    ]
}